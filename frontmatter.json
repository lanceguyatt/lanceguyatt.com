      "title": "cannonburyyard"
    },
    {
      "path": "[[workspace]]/src/content/work/associated-press",
      "title": "associated-press"
    },
    {
      "path": "[[workspace]]/src/content/tag",
      "title": "tag"
    },
    {
      "path": "[[workspace]]/src/content/work/bauer-media",
      "title": "bauer-media"
    },
    {
      "path": "[[workspace]]/src/content/work/hearst-content-agency",
      "title": "hearst-content-agency"
    },
    {
      "path": "[[workspace]]/src/content/work/futurestate-design",
      "title": "futurestate-design"
    },
    {
      "path": "[[workspace]]/src/content/work/hearst-spain",
