---
import Workbench from '~/layouts/Workbench.astro'
import { SchemaWebPage } from '~/components/index.astro'
import Work from '~/containers/Work.astro'

import { frontmatter } from '../../content/page/work.mdx'

const { name, description } = frontmatter
const page = {
  title: name,
  description,
  url: '/work',
}
---

<Workbench {...page}>
  <SchemaWebPage slot="head" name={name} {...page} />
  <Work />
</Workbench>
