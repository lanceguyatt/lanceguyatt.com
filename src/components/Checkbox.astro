---
import type { HTMLAttributes } from 'astro/types'

type Props = HTMLAttributes<'input'> & {}

const { class: className, ...props } = Astro.props
---

<input
  type="checkbox"
  class:list={[
    'h-22px w-26px bg-checkbox focus:bg-primary appearance-none outline-0 checked:bg-[-52px]',
    className
  ]}
  {...props}
/>
