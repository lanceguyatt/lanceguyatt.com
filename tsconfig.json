{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["src/components/*"],
      "@/config": ["src/config"],
      "@/containers/*": ["src/containers/*"],
      "@/content/*": ["src/content/*"],
      "@/layouts/*": ["src/layouts/*"],
      "@/styles/*": ["src/styles/*"],
      "@/schema/*": ["src/schema/*"]
    }
  }
}
